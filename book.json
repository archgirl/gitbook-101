{
    "plugins": [
        "edit-link",
        "git-author"
    ],
    "pluginsConfig": {
        "edit-link": {
            "base": "https://github.com/archgirl/gitbook-101/tree/master",
            "label": "Edit This Page"
        },
        "git-author":{
              "position": "bottom",
              "modifyTpl": "Last modified by {user} {timeStamp}",
              "createTpl": "Created by {user} {timeStamp}",
              "timeStampFormat": "YYYY-MM-DD HH:mm:ss"
        }
    }
}